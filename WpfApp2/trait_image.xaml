        <Image Margin="962,255,105.2,384.8" Source="Images_Affichage/blur.png" Stretch="Fill"/>
        <Image Margin="766,282,329.2,390.8" Source="Images_Affichage/streamline-icon-interface-edit-select-area-rectangle-dash@100x100.PNG" Stretch="Fill"/>
