        <Button Style="{StaticResource RoundButton}" Content="Rétrecir" HorizontalAlignment="Left" Margin="954,154,0,0" VerticalAlignment="Top" Width="150" Background="#FF4C4E4B" Height="70" Click="Button_retrecir" FontWeight="Bold" FontSize="16"/>
